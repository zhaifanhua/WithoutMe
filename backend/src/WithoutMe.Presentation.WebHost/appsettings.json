{
  "AllowedHosts": "*",
  "IsDemoMode": false,
  "Port": 9708,
  "Cors": {
    "IsEnabled": true,
    "PolicyName": "XiHan",
    // 支持的域名端口，端口号后不可带/符号
    "Origins": [
      "http://127.0.0.1:9708"
    ],
    // 支持的请求头
    "Headers": [
      "X-Access-Control-Expose-Headers",
      "X-Pagination"
    ]
  },
  "Swagger": {
    "RoutePrefix": "",
    // API文档动态显示，取值范围：[ "All", "Display", "Manage", "Authorize", "Common", "Test" ]
    "PublishGroup": [
      "All",
      "Display",
      "Manage",
      "Authorize",
      "Common",
      "Test"
    ]
  },
  "MiniProfiler": {
    "IsEnabled": true
  },
  "Auth": {
