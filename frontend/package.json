{
  "$schema": "https://json.schemastore.org/jsconfig",
  "name": "WithoutMe",
  "private": false,
  "version": "0.2.4",
  "revisionDate": "20240813",
  "type": "module",
  "description": "WithoutMe project. An application that includes personal blogs, smart internet, open source projects, file synchronization, etc. built on Vue.",
  "author": {
    "name": "ZhaiFanhua",
    "email": "zhaifanhua@foxmail.com",
    "url": "https://www.zhaifanhua.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ZhaiFanhua/WithoutMe",
  "repository": {
    "url": "https://github.com/ZhaiFanhua/WithoutMe.git"
  },
  "bugs": {
    "url": "https://github.com/ZhaiFanhua/WithoutMe/issues"
  },
  "keywords": [
    "Vue",
    "Vite",
    "TypeScript",
    "WithoutMe"
  ],
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite dev",
    "debug": "vite dev --debug",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,vue,css,scss,less,json,md}\"",
    "update-pkg": "ncu -u",
    "cache-verify": "npm cache verify",
    "clean-lock": "node ./scripts/clean-lock.js",
    "clean-modules": "node ./scripts/clean-modules.js",
    "clean-dist": "node ./scripts/clean-dist.js",
    "clean-node": "pnpm run clean-lock && pnpm run clean-modules",
    "reinstall": "pnpm run clean-node && pnpm run bootstrap",
    "rebuild": "pnpm run clean-dist && pnpm run build"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "axios": "^1.7.9",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0",
    "xihan-ui": "^0.8.14"
  },
  "devDependencies": {
    "@iconify/vue": "^4.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.5.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "npm-check-updates": "^17.1.14",
    "prettier": "^3.5.0",
    "sass": "^1.84.0",
    "sass-lint": "^1.13.1",
    "stylelint": "^16.14.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2"
  },
  "engines": {
    "node": "^23.7.0",
    "pnpm": "^10.2.0"
  },
  "packageManager": "pnpm@10.3.0"
}
